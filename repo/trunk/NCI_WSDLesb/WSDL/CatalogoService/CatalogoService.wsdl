<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="CatalogoService"
	targetNamespace="http://mx.profuturo/iib/nci/Administracion/CatalogoService/v1"
	xmlns:tns="http://mx.profuturo/iib/nci/Administracion/CatalogoService/v1" 
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsd2="http://mx.profuturo/iib/nci/Administracion/CatalogoService/v1/types"
	xmlns:iibe="http://mx.profuturo/iib/iibException/IIBException">

	<wsdl:types>
		<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:import
				namespace="http://mx.profuturo/iib/nci/Administracion/CatalogoService/v1/types"
				schemaLocation="CatalogoService.xsd">
			</xsd:import>
			<xsd:import
				namespace="http://mx.profuturo/iib/iibException/IIBException"
				schemaLocation="../frameworkiib/iibException/IIBException.xsd">
			</xsd:import>
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="consultarCatalogoRequest">
		<wsdl:part element="xsd2:consultarCatalogoRequest" name="parameters">
		</wsdl:part>
	</wsdl:message>
	<wsdl:message name="consultarCatalogoResponse">
		<wsdl:part element="xsd2:consultarCatalogoResponse" name="parameters">
		</wsdl:part>
	</wsdl:message>
	<wsdl:message name="faultMsg">
	    <wsdl:part element="iibe:iibException" name="parameters"/>
  	</wsdl:message>
	
	<wsdl:portType name="CatalogoPortType">
		<wsdl:operation name="consultarCatalogo">
			<wsdl:input message="tns:consultarCatalogoRequest"/>
			<wsdl:output message="tns:consultarCatalogoResponse"/>
			<wsdl:fault message="tns:faultMsg" name="fault"/>
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="CatalogoBinding" type="tns:CatalogoPortType">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="consultarCatalogo">
			<soap:operation soapAction="urn:consultarCatalogo" style="document"/>
			<wsdl:input>
				<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="CatalogoService">
		<wsdl:port binding="tns:CatalogoBinding" name="CatalogoPort">
			<soap:address location="http://localhost/iib/nci/Administracion/CatalogoService/v1" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>