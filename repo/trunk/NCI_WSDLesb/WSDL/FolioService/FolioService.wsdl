			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="FolioService">
		<wsdl:port binding="tns:FolioBinding" name="FolioPort">
			<soap:address location="http://localhost/iib/nci/BitacoraProcesos/FolioService/v1" />
		</wsdl:port>
