<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="FolioService"
	targetNamespace="http://mx.profuturo/iib/nci/BitacoraProcesos/FolioService/v1"
	xmlns:tns="http://mx.profuturo/iib/nci/BitacoraProcesos/FolioService/v1" 
	xmlns:sch="http://mx.profuturo/iib/nci/BitacoraProcesos/FolioService/v1/types"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:iibe="http://mx.profuturo/iib/iibException/IIBException">

	<wsdl:types>
